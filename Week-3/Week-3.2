import java.util.*;
class prog{
    public static void main(String [] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int [] arr=new int[n];
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
        }
        int max=arr[0];
        for(int i=0;i<n;i++){
            if(arr[i]>max){
                max=arr[i];
            }
        }
        for(int i=0;i<n;i++){
            arr[i]=arr[i]-max;
        }
        for(int i=0;i<n;i++){
            arr[i]=arr[i]*max;
        }
        int sum=0;
        for(int i:arr){
            System.out.print(i+" ");
        }
    }
}
