import java.util.*;
class Student{
    String name;
    int Roll_no=0;
    Student(){
        this.name=null;
        this.Roll_no=0;
    }
    Student(String name){
        this.name=name;
        this.Roll_no=0;
    }
    Student(String name,int Roll_no){
        this.name=name;
        this.Roll_no=Roll_no;
    }
}
public class prog{
    public static void main(String [] args){
        Scanner sc = new Scanner(System.in);
        Student s1=new Student();
        Student s2= new Student("Rajalakshmi");
        Student s3=new Student("Lakshmi",101);
        System.out.println("No-arg constructor is invoked");
        System.out.println("1 arg constructor is invoked");
        System.out.println("2 arg constructor is invoked");
        System.out.println("Name =null , Roll no = 0");
        System.out.println("Name =Rajalakshmi , Roll no = 0");
        System.out.println("Name =Lakshmi , Roll no = 101");
    }
}
